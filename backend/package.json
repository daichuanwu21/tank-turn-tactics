{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node build/index.js",
    "start:watch": "nodemon --watch src -e ts --exec \"npm run build && npm run start\"",
    "clean": "rimraf build",
    "build:release": "npm run clean && tsc -p tsconfig.release.json"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.18",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "pino-http": "^8.3.3"
  }
}
